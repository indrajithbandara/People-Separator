@{
    ViewBag.Title = "DisplayData";
    List<List<PipSep.Models.ApplicationCV>> result = ViewBag.results;
    List<PipSep.Models.ApplicationCV> remained = ViewBag.remained;
}

<h2>DisplayData</h2>
@*<p style="font-size:200%">This data is "very" incorrect. Sorry.</p>*@
<h1>We have them in the groups</h1>
<ul>
@for (int i=0; i<result.Count;++i)
{
    <li>
    Group @(i+1)
    <ul>
    @foreach (var item in result[i])
	{
        <li>
		 @item.UserName
        </li>
	}
    </ul>
    </li>
}
</ul>

@if (remained.Count > 0)
{
<h1>Not separated</h1>
}
<ul>
@foreach (var i in remained)
{
    <li>@i.UserName</li>
}
</ul>